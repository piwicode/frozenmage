            <string>var to_time = get_timer();
var time_spent = string(round((to_time - from_time) / 1000 / 1000));
show_debug_message("Analytics: Send " + time_spent +
  " secs spent in " + room_get_name(room));
var payload =
  "v=1&amp;t=event" + 
  // game name with spaces replaced by underscores.
  "&amp;an=" + game_display_name +
  "&amp;av=" + GM_version + // game version
  "&amp;tid=" + tracking_id +
  "&amp;cid=" + client_id +
  "&amp;ec=lvl_completed" + 
  "&amp;ea=" + room_get_name(room) +
  "&amp;ev=" + string(time_spent);

var url = "https://www.google-analytics.com/collect";
http_post_string(url, payload);

// Surprisingly room start is not called on room restart, but room end is.
// We want to reset the from_time so that a time span is not reported 
// multiple times.
from_time = to_time;
</string>
