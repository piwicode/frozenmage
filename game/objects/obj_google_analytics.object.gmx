            <string>/// Read existing clientid, or generate and persist one.
var client_id_file_name = "client_id.txt"
if (file_exists(client_id_file_name)) { 
  var file_id = file_text_open_read(client_id_file_name);
  client_id = file_text_read_string(file_id);
  file_text_close(file_id);
  show_debug_message("Analytics: Read persisted client id " + client_id);
} else  {  
  client_id = string(irandom($fffffffffffffff));
  var file_id = file_text_open_write(client_id_file_name);  
  file_text_write_string(file_id, client_id);
  file_text_close(file_id); 
  show_debug_message("Analytics: Generate and persist client id " + client_id);
}

tracking_id = "UA-86788013-2";

// Capture the start time for the very first room displayed.
from_time = get_timer();

</string>
